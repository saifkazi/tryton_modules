<form cursor="customer">
    <label name="reference"/>
    <field name="reference"/>
    <label name="number"/>
    <field name="number"/>
    <label name="customer"/>
    <field name="customer"/>
    <label name="address"/>
    <field name="address"/>
    <label name="date"/>
    <field name="date"/>
    <notebook>
        <page string="Complaint" id="complaint">
            <label name="company"/>
            <field name="company"/>
            <label name="employee"/>
            <field name="employee"/>
            <label name="type"/>
            <field name="type" widget="selection"/>
            <label name="origin"/>
            <field name="origin"/>
            <separator name="description" colspan="2"/>
            <newline/>
            <field name="description" colspan="2"/>
            <field name="actions" colspan="2"/>
        </page>
    </notebook>
    <label name="state"/>
    <field name="state"/>
    <group col="-1" colspan="2" id="buttons">
        <button name="cancel" icon="tryton-cancel"/>
        <button name="draft"/>
        <button name="wait" icon="tryton-forward"/>
        <button name="approve" icon="tryton-forward"/>
        <button name="reject" icon="tryton-close"/>
        <button name="process" icon="tryton-launch"/>
    </group>
</form>
